<h1 class="text-center">My Playlists</h1>

<div class="row">
  <div class="col-sm-4 col-sm-offset-4">
    <%= link_to "New Playlist", new_user_playlist_path, class: "btn btn-lg btn-primary btn-block" %>
  </div>
</div>
<br/>

<div class="row">
  <% @playlists.each do |playlist| %>
    <%= render 'playlist', playlist: playlist %>
  <% end %>
</div>